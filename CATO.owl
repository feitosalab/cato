    <!-- http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#anatomical_quality -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#anatomical_quality">
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An organismal quality inhering in a bearer by virtue of the bearer&apos;s phenotypic organization.</definition>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">anatomical quality</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#apterous -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#apterous">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#anatomical_quality"/>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An anatomical quality inhering in a bearer by virtue of the bearer&apos;s absence of axillary sclerites.</definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#biological_sex_quality -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#biological_sex_quality">
        <database_cross_reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PATO:0000047</database_cross_reference>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An organismal quality inhering in a bearer by virtue of the bearer&apos;s ability to undergo sexual reproduction in order to differentiate the individuals or types involved.</definition>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">biological sex</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#female -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#female">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#&apos;phenotypic_sex&apos;"/>
        <database_cross_reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PATO:0000383</database_cross_reference>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A biological sex quality inhering in an individual or a population that only produces gametes that can be fertilised by male gametes.</definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#fertile -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#fertile">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#reproductive_quality"/>
        <database_cross_reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PATO:0000955</database_cross_reference>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A fertility quality inhering in a bearer by virtue of the bearer&apos;s being capable of initiating, sustaining, or supporting reproduction.</definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#male -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#male">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#&apos;phenotypic_sex&apos;"/>
        <database_cross_reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PATO:0000384</database_cross_reference>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A biological sex quality inhering in an individual or a population whose sex organs contain only male gametes.</definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#reproductive_quality -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#reproductive_quality">
        <database_cross_reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PATO:0000186</database_cross_reference>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An organismal quality inhering in a bearer by virtue of the bearer&apos;s behavior aggregate of the responses or reactions or movements in a given situation.</definition>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">reproductive quality</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#sterile -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#sterile">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#reproductive_quality"/>
        <database_cross_reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PATO:0000956</database_cross_reference>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A fertility quality inhering in a bearer by virtue of the bearer&apos;s being incapable of initiating, sustaining, or supporting reproduction.</definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#winged -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#winged">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#anatomical_quality"/>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An anatomical quality inhering in a bearer by virtue of the bearer&apos;s having developed axillary sclerites.</definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#&apos;phenotypic_sex&apos; -->

    <owl:Class rdf:about="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#&apos;phenotypic_sex&apos;">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/thiago/ontologies/2018/5/phenotypic_mosaic#biological_sex_quality"/>
        <database_cross_reference rdf:datatype="http://www.w3.org/2001/XMLSchema#string">PATO:0001894</database_cross_reference>
        <definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A biological sex quality inhering in an individual based upon phenotypic composition of mature individuals.</definition>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">phenotypic sex</rdfs:label>
    </owl:Class>
